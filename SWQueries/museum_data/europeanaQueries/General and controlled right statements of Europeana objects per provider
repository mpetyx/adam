# General and controlled right statements of Europeana objects per provider
		PREFIX edm: <http://www.europeana.eu/schemas/edm/>
		PREFIX dc: <http://purl.org/dc/elements/1.1/>
		PREFIX ens:    <http://www.europeana.eu/schemas/edm/>

		SELECT DISTINCT ?Provider ?Copyright ?CreativeCommonsRights ?CHO
		WHERE {

      		  ?EuropeanaObject edm:provider ?Provider ;
                         dc:rights ?Copyright ;
                         edm:rights ?CreativeCommonsRights .
			  ?EuropeanaObject  ens:aggregatedCHO ?CHO
		}