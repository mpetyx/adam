#Enrichment statements produced by Europeana for objects provided by institutions from the United Kingdom
		PREFIX ore: <http://www.openarchives.org/ore/terms/>
        PREFIX edm: <http://www.europeana.eu/schemas/edm/>

        SELECT DISTINCT ?CHO ?enrichment
        WHERE {

                 ?a edm:country "united kingdom" .
                 ?proxy ore:proxyIn ?a ;
                        edm:hasMet ?enrichment.

                ?proxy ore:proxyFor ?CHO.
        }
        LIMIT 100